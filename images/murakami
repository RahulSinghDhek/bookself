margin: 0;
padding: 0;
}
h1, h2, h3, h4 {
font-family: Arial,sans-serif;
text-rendering: optimizelegibility;
padding-bottom: 4px;
}
h1:last-child, h2:last-child, h3:last-child, h4:last-child {
padding-bottom: 0;
}
