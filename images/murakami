'frontImageWidth' : 225,
'frontImageHeight' : 346,
'frontImageAspectRatio' : 0.6502890173410405,
'imageGalleryData' : [{"mainUrl":"https://images-na.ssl-images-amazon.com/images/I/91fJxgs69QL.jpg","dimensions":[1520,2348],"thumbUrl":"https://images-na.ssl-images-amazon.com/images/I/91fJxgs69QL._AC_SX75_CR,0,0,75,75_.jpg"},{"mainUrl":"https://images-na.ssl-images-amazon.com/images/I/51zCE0pCmRS.jpg","dimensions":[1337,357],"thumbUrl":"https://images-na.ssl-images-amazon.com/images/I/51zCE0pCmRS._AC_SY75_CR,0,0,75,75_.jpg"},{"mainUrl":"https://images-na.ssl-images-amazon.com/images/I/31xvzGS4v+S.jpg","dimensions":[500,180],"thumbUrl":"https://images-na.ssl-images-amazon.com/images/I/31xvzGS4v+S._AC_SY75_CR,0,0,75,75_.jpg"}],
